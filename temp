<script type="text/javascript">
    jQuery(document).ready(function() {        
        var currentTime = moment().tz("America/Los_Angeles");
        var currentDay = currentTime.day();
        var currentHour = currentTime.hour();
        
        if (currentDay === 0 && currentHour <= 19) {
            alert('Every Sunday from 9 a.m. - 7 p.m., maintenance activities are performed that can impact PeMS. Please be aware that these activities can sometimes prevent users from logging in, and sometimes interfere with report generation, causes reports to be slow or time out. We apologize for any inconvenience.');
        }
    });
</script>

