<script type="text/javascript">

YAHOO.util.Event.onDOMReady(function() {

    YAHOO.util.Event.addListener('username', 'focus', function() {
        if (this.value == 'Username') {
            YAHOO.util.Dom.setStyle(this, 'color', '#000000');
            this.value = '';
        }
        return true;
    });

    YAHOO.util.Event.addListener('username', 'blur', function() {
        if (this.value.length == 0) {
            YAHOO.util.Dom.setStyle(this, 'color', '#d0d0d0');
            this.value = 'Username';
        }
        return true;
    });
});

</script>



